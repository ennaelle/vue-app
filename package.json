{
  "name": "vue-app",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint . --ext .js,.vue",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:unit:report": "vitest run --reporter=junit --outputFile=test-results.xml",
    "test:security": "npm audit --audit-level=high",
    "coverage": "vitest run --coverage.enabled=true --coverage.reporter=cobertura --coverage.reporter=text"
  },
  "dependencies": {
    "vue": "^3.5.22",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.36.0",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vue": "^10.5.0",
    "jsdom": "^27.0.0",
    "vite": "^7.1.9",
    "vitest": "^3.2.4"
  }
}
